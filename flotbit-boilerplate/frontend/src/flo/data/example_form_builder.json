{
  "formId": "unique-form-id",
  "title": "Multi-Page Form",
  "description": "A form with conditional logic and multiple pages",
  "layout_theme": "",
  "version": "1.0",
  "settings": {
    "allowBack": true,
    "showProgress": true,
    "saveProgress": true,
    "submitOnce": false
  },
  "pages": [
    {
      "title": "page 1",
      "layout_type": "fullscreen",
      "fields": [
        {
          "title": "Give us rating of our work?",
          "discription": "Description (optional)",
          "required": false,
          "type": "stepped_progressbar",
          "config": {
            "total": 5,
            "defaultValue": 3,
            "showLabels": true
          }
        }
      ]
    },
    {
      "title": "page 2",
      "layout_type": "fullscreen",
      "fields": [
        {
          "title": "Give us rating of our work?",
          "discription": "Description (optional)",
          "required": false,
          "type": "star_rating",
          "config": {
            "total": 5,
            "defaultValue": 3
          }
        }
      ]
    },
    {
      "title": "page 3",
      "layout_type": "fullscreen",
      "fields": [
        {
          "title": "Enter your age?",
          "discription": "Description (optional)",
          "required": false,
          "type": "number",
          "config": {
            "defaultValue": 3,
            "min": 5,
            "max": 18
          }
        }
      ]
    },
    {
      "title": "page 4",
      "layout_type": "fullscreen",
      "fields": [
        {
          "title": "Select from given choices?",
          "discription": "Description (optional)",
          "required": false,
          "type": "checkbox",
          "config": {
            "options": [
              {
                "value": 0,
                "label": "Choice 1"
              },
              {
                "value": 1,
                "label": "Choice 2"
              }
            ],
            "defaultValue": [
              0
            ]
          }
        }
      ]
    },
    {
      "title": "page 5",
      "layout_type": "fullscreen",
      "fields": [
        {
          "title": "Select from given choices?",
          "discription": "Description (optional)",
          "required": false,
          "type": "radio",
          "config": {
            "options": [
              {
                "value": 0,
                "label": "Choice 1"
              },
              {
                "value": 1,
                "label": "Choice 2"
              }
            ],
            "defaultValue": 0
          }
        }
      ]
    },
    {
      "title": "page 6",
      "layout_type": "fullscreen",
      "fields": [
        {
          "title": "Upload Document",
          "discription": "Description (optional)",
          "required": false,
          "type": "file_upload",
          "config": {
            "maxSize": 10,
            "acceptedFileTypes": ".pdf,.jpg,.jpeg,.png,.doc,.docx"
          }
        }
      ]
    },
    {
      "title": "page 7",
      "layout_type": "fullscreen",
      "fields": [
        {
          "title": "How's your experience?",
          "discription": "Description (optional)",
          "required": false,
          "type": "emojis_review",
          "config": {
            "defaultValue": 1
          }
        }
      ]
    },
    {
      "title": "page 8",
      "layout_type": "fullscreen",
      "fields": [
        {
          "title": "How's your experience?",
          "discription": "Description (optional)",
          "required": false,
          "type": "yes_or_no",
          "config": {
            "defaultValue": 1
          }
        }
      ]
    },
    {
      "title": "page 9",
      "layout_type": "fullscreen",
      "fields": [
        {
          "title": "How's your experience?",
          "discription": "Description (optional)",
          "required": false,
          "type": "detailed_options",
          "config": {
            "options": [
              {
                "value": 1,
                "text": "Ans 1"
              },
              {
                "value": 2,
                "text": "Ans 2"
              },
              {
                "value": 3,
                "text": "Ans 3"
              },
              {
                "value": 4,
                "text": "Ans 4"
              }
            ],
            "defaultValue": 1
          }
        }
      ]
    }
  ],
  "globalActions": [
    {
      "actionId": "auto-save",
      "type": "auto_save",
      "trigger": {
        "event": "field_change",
        "debounce": 2000
      }
    }
  ],
  "submissions": {
    "endpoint": "/api/forms/submit",
    "method": "POST",
    "headers": {
      "Content-Type": "application/json"
    },
    "successMessage": "Form submitted successfully!",
    "errorMessage": "There was an error submitting the form. Please try again."
  }
}